# Azure Cosmos DB Configuration
# For production, use Managed Identity (leave COSMOS_DB_CONNECTION_STRING empty)
# For development, you can use the connection string
COSMOS_DB_ENDPOINT=https://your-cosmosdb-account.documents.azure.com:443/
COSMOS_DB_CONNECTION_STRING=
COSMOS_DB_DATABASE=HealthcareDB
COSMOS_DB_CONTAINER=Patients

# Azure Communication Services
ACS_CONNECTION_STRING=your-acs-connection-string

# Azure Speech Services
SPEECH_KEY=your-speech-service-key
SPEECH_REGION=your-speech-service-region

# Bot Framework
MicrosoftAppId=your-bot-app-id
MicrosoftAppPassword=your-bot-app-password
MicrosoftAppTenantId=your-tenant-id
